{
  "name": "music-bot",
  "version": "1.0.0",
  "private": true,
  "description": "Discord music bot example built with discord-player.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "dev": "pnpm run build && node dist/index.js",
    "migrate": "prisma migrate dev --name init",
    "reset": "prisma migrate reset --force",
    "migrate:prod": "prisma migrate deploy",
    "generate": "prisma generate",
    "studio": "prisma studio"
  },
  "imports": {
    "#bot/*": "./dist/*.js"
  },
  "keywords": [],
  "author": "twlite",
  "license": "MIT",
  "dependencies": {
    "@discord-player/extractor": "4.4.6-dev.0",
    "@prisma/client": "5.5.2",
    "@trpc/server": "^10.43.1",
    "commandkit": "^0.1.6",
    "discord-player": "6.6.7-dev.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "mediaplex": "^0.0.8",
    "prisma": "^5.5.2",
    "youtube-ext": "^1.1.16"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "rimraf": "^5.0.5",
    "tsconfig": "workspace:*",
    "typescript": "^5.2.2"
  }
}